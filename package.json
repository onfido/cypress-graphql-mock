{
  "name": "cypress-graphql-mock",
  "description": "Mock out a GraphQL schema from the client",
  "version": "0.1.1",
  "scripts": {
    "dev": "tsc -w",
    "prepublish": "tsc",
    "server": "node test-server/index.js",
    "cypress:open": "cypress open",
    "cypress:all": "npm-run-all --parallel server cypress:open"
  },
  "main": "dist",
  "types": "src",
  "files": [
    "dist/",
    "src/",
    "yarn.lock",
    "README.md"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": {
    "name": "Tim Griesser"
  },
  "dependencies": {
    "@types/graphql": "^14.2.0",
    "@types/node-fetch": "^2.3.4",
    "graphql": "^14.3.1",
    "graphql-tools": "^4.0.3",
    "tslib": "^1.9.3"
  },
  "peerDependencies": {
    "cypress": "^3.1.3"
  },
  "devDependencies": {
    "apollo-server": "^2.6.1",
    "cypress": "3.3.1",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "typescript": "^3.2.2"
  }
}
